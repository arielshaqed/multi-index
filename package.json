{
  "name": "multi-index",
  "version": "0.0.3",
  "description": "Multiple indexes on a single collection of objects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "ava dist/test/**.js",
    "coverage": "nyc ava dist/test/**.js --reporter=text-lcov"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "**/test/**"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binaris/multi-index.git"
  },
  "keywords": [
    "container",
    "multi_index",
    "multiple-indices",
    "indices",
    "multi",
    "index",
    "map"
  ],
  "author": "Ariel Shaqed (Scolnicov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binaris/multi-index/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^3.8.2",
    "coveralls": "^3.1.0",
    "nyc": "^15.0.1",
    "typescript": "^3.7.3"
  }
}
